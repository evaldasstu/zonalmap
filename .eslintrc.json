{
    "extends": [
        "airbnb",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:jest/recommended",
        "plugin:jest/style"
    ],
    "plugins": [
        "jsx-a11y",
        "react",
        "jest"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/no-danger": "off",
        "react/jsx-one-expression-per-line": "off"
    },
    "env": {
        "browser": true
    },
    "parser": "babel-eslint"
}
